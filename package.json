  "author": "Shogoro Yoshida",
  "scripts": {
    "audit:moderate": "yarn audit",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ],
    ".eslintrc.js": [
      "eslint --fix"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "yarn.lock": [
      "audit:moderate"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "dayjs": "^1.9.8",
    "next": "^10.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^5.3.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.40.0",
    "typescript": "3.8.3"
  }
